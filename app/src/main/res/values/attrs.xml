<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="IntensifyImageView">
        <attr name="scaleType" format="enum">
            <enum name="fitAuto" value="1" />
            <enum name="fitCenter" value="2" />
        </attr>
        <attr name="animateScaleType" format="boolean" />
        <attr name="maximumScale" format="float" />
        <attr name="minimumScale" format="float" />
        <attr name="scale" format="float" />
    </declare-styleable>

    <declare-styleable name="SubsamplingScaleImageView">
        <attr name="src" format="reference" />
        <attr name="assetName" format="string" />
        <attr name="panEnabled" format="boolean" />
        <attr name="zoomEnabled" format="boolean" />
        <attr name="quickScaleEnabled" format="boolean" />
        <attr name="tileBackgroundColor" format="color" />
    </declare-styleable>

</resources>